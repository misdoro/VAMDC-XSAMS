<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
elementFormDefault="unqualified" attributeFormDefault="unqualified"
xmlns:typ="http://www-amdis.iaea.org/xsams/0.3/types"
xmlns:coll="http://www-amdis.iaea.org/xsams/0.3/processCollisional"
targetNamespace="http://www-amdis.iaea.org/xsams/0.3/processCollisional"

>
	<xs:import namespace="http://www-amdis.iaea.org/xsams/0.3/types" schemaLocation="typesAttributes.xsd"/>
	<xs:element name="Collisions" type="coll:CollisionsType">
		<xs:annotation>
			<xs:documentation>List of collisional processes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CollisionsType">
		<xs:complexContent>
			<xs:extension base="typ:PrimaryType">
				<xs:sequence>
					<xs:element name="CollisionalTransition" type="coll:CollisionalTransitionType" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A specific collisional transition</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CollisionalTransitionType">
		<xs:complexContent>
			<xs:extension base="typ:PrimaryType">
				<xs:sequence>
					<xs:element name="ProcessClass" type="coll:CollisionalProcessClassType">
						<xs:annotation>
							<xs:documentation>Collisional process</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Reactants" type="typ:ReactantsType">
						<xs:annotation>
							<xs:documentation>List of reacting systems</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IntermediateStates" type="typ:ProductsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>List of intermediate state</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Products" type="typ:ProductsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>List of final states</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Threshold" type="typ:DataType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reaction threshold</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataSets" type="typ:DataSetsType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CollisionalProcessClassType">
		<xs:sequence>
			<xs:element name="UserDefinition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description of the process</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="coll:CodeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A 4-letter code describing various processes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IAEACode" type="coll:IAEACodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From the "IAEA Classification of Processes", October 2003</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IAEACodeType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="EGN"/>
			<xs:enumeration value="EAS"/>
			<xs:enumeration value="EBS"/>
			<xs:enumeration value="EDX"/>
			<xs:enumeration value="EEL"/>
			<xs:enumeration value="ELB"/>
			<xs:enumeration value="ETS"/>
			<xs:enumeration value="EDT"/>
			<xs:enumeration value="EFL"/>
			<xs:enumeration value="EEX"/>
			<xs:enumeration value="EIN"/>
			<xs:enumeration value="EMI"/>
			<xs:enumeration value="ENI"/>
			<xs:enumeration value="EMT"/>
			<xs:enumeration value="EUP"/>
			<xs:enumeration value="EDP"/>
			<xs:enumeration value="EIP"/>
			<xs:enumeration value="ERC"/>
